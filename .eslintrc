{
    "parser": "babel-eslint",
    "env": {
        "browser": true
    },
    "plugins": [
        "react",
        "react-native"
    ],
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "rules": {
        "no-undef": "off",
        "semi": [
            "error",
            "never"
        ],
        "indent": [
            "error",
            2
        ],
        "react/prop-types": "off",
        "react/display-name": "off",
        "no-console": "off"
    }
}